{
  "name": "oxygen-cli",
  "version": "1.14.2",
  "description": "Automation framework for testing modern applications",
  "main": "./build/index.js",
  "bin": {
    "oxygen": "./build/lib/cli.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/oxygenhq/oxygen.git"
  },
  "keywords": [
    "selenium",
    "appium",
    "webdriver",
    "testing",
    "automation",
    "qa"
  ],
  "author": {
    "name": "CloudBeat Limited",
    "email": "info@oxygenhq.org",
    "url": "http://oxygenhq.org"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/oxygenhq/oxygen/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "babel-plugin-source-map-support": "2.1.3",
    "cross-env": "7.0.2",
    "doctrine": "3.0.0",
    "eslint": "6.8.0",
    "husky": "4.2.5",
    "md-reverse": "1.0.0",
    "npm-run-all": "4.1.5"
  },
  "dependencies": {
    "@applitools/eyes-webdriverio": "5.23.1",
    "@lambdatest/node-rest-client": "1.0.0",
    "@wdio/cli": "6.6.6",
    "@wdio/devtools-service": "6.6.6",
    "@wdio/sync": "6.6.4",
    "async": "3.2.0",
    "chai": "4.2.0",
    "chrome-har": "0.11.12",
    "config": "3.3.3",
    "csv-parse": "4.12.0",
    "cucumber": "6.0.5",
    "deasync": "0.1.20",
    "decache": "4.6.0",
    "easyxml": "2.0.1",
    "ejs": "3.0.2",
    "esm": "3.2.25",
    "fibers": "4.0.2",
    "glob": "7.1.6",
    "glob-to-regexp": "0.4.1",
    "imap-simple": "5.0.0",
    "is-glob": "4.0.1",
    "jimp": "0.16.1",
    "junit-report-builder": "2.1.0",
    "lodash": "4.17.20",
    "minimist": "1.2.5",
    "mockery": "2.1.0",
    "moment": "2.29.1",
    "ox-chrome-remote-interface": "0.28.1",
    "oxygen-logger": "0.5.0",
    "pdfreader": "1.0.4",
    "perfecto-reporting": "2.5.3",
    "request": "2.88.2",
    "saucelabs": "3.0.1",
    "serialport": "9.0.2",
    "soap": "0.35.0",
    "source-map-support": "0.5.19",
    "stacktrace-parser": "0.1.10",
    "testdouble": "3.16.1",
    "testingbot-api": "1.0.7",
    "text-to-image": "2.4.1",
    "twilio": "3.52.0",
    "webdriverio": "6.6.6",
    "when": "3.7.8",
    "xlsx": "0.16.8"
  },
  "optionalDependencies": {
    "odbc": "2.3.5"
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf ./build",
    "compile": "babel src/ -d build/ --config-file ./.babelrc --copy-files",
    "test": "run-s test:*",
    "test:lint": "eslint src test",
    "test:unit": "jest",
    "prepublishOnly": "npm run build && node tools/dos2unix src/lib/cli.js",
    "postpublish": "node tools/unix2dos src/lib/cli.js",
    "lint": "eslint \"src/**/*.js\" \"tools/**/*.js\"",
    "apidoc": "node -r esm tools/apidoc",
    "prod": "node ./build/lib/cli.js",
    "dev": "cross-env DEBUG=true NODE_ENV=development npm run compile && npm run prod"
  },
  "engines": {
    "node": ">=10.18.1 <13.0"
  }
}
