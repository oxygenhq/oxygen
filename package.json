{
  "name": "oxygen-cli",
  "version": "0.54.0-beta.4",
  "description": "Automation framework for Web & Mobile applications",
  "main": "./lib/oxygen.js",
  "bin": {
    "oxygen": "./build/lib/cli.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/oxygenhq/oxygen.git"
  },
  "keywords": [
    "selenium",
    "appium",
    "webdriver",
    "testing",
    "automation",
    "qa"
  ],
  "author": {
    "name": "CloudBeat Limited",
    "email": "info@oxygenhq.org",
    "url": "http://oxygenhq.org"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/oxygenhq/oxygen/issues"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/node": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-optional-catch-binding": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/preset-env": "7.5.5",
    "babel-plugin-source-map-support": "2.1.1",
    "source-map-support": "0.5.16",
    "cross-env": "6.0.3",
    "doctrine": "3.0.0",
    "eslint": "6.5.1",
    "npm-run-all": "4.1.5"
  },
  "dependencies": {
    "@applitools/eyes-webdriverio": "5.7.2",
    "@babel/cli": "7.2.3",
    "@babel/register": "7.5.5",
    "@wdio/config": "5.15.3",
    "@wdio/sync": "5.15.7",
    "@wdio/utils": "5.15.1",
    "async": "3.1.0",
    "chai": "4.2.0",
    "chrome-har": "0.11.2",
    "config": "3.2.2",
    "csv-parse": "4.4.7",
    "cucumber": "5.1.0",
    "deasync": "0.1.15",
    "easyxml": "2.0.1",
    "ejs": "2.7.1",
    "fibers": "4.0.1",
    "glob": "7.1.4",
    "glob-to-regexp": "0.4.1",
    "globule": "1.2.1",
    "imap-simple": "4.3.0",
    "is-glob": "4.0.1",
    "junit-report-builder": "1.3.3",
    "lodash": "4.17.15",
    "minimist": "1.2.0",
    "mockery": "2.1.0",
    "moment": "2.24.0",
    "ox-chrome-remote-interface": "0.27.2",
    "oxygen-logger": "0.3.0",
    "pdfreader": "1.0.4",
    "request": "2.88.0",
    "serialport": "7.1.5",
    "soap": "0.29.0",
    "strip-comments": "1.0.2",
    "twilio": "3.35.0",
    "webdriverio": "5.15.6",
    "when": "3.7.8",
    "xlsx": "0.15.1"
  },
  "optionalDependencies": {
    "odbc": "1.4.6"
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf ./build",
    "compile": "babel src/ -d build/ --config-file ./babel.config.js",
    "test": "run-s test:*",
    "test:eslint": "eslint src test",
    "test:unit": "jest",
    "prepublishOnly": "npm run eslint && npm run build &&node tools/dos2unix src/lib/cli.js",
    "postpublish": "node tools/unix2dos src/lib/cli.js",
    "eslint": "eslint \"Gruntfile.js\" \"src/lib/**/*.js\" \"src/errors/**/*.js\" \"src/model/**/*.js\" \"tools/**/*.js\"",
    "apidoc": "node tools/apidoc",
    "prod": "node ./build/lib/cli.js",
    "dev": "cross-env DEBUG=true babel-node --config-file ./babel.config.js ./src/lib/cli.js",
    "debug": "cross-env DEBUG=true babel-node --debug=5858 --presets @babel/preset-env ./lib/cli.js"
  }
}
