{
    "presets": [
        [
            "@babel/preset-env", {
                "targets": {
                    "node": 10
                }
            },
            "@babel/preset-typescript"
        ]
    ],
    "plugins": [
    ],
    "env": {
        "development": {
            "sourceMaps": "inline",
            "plugins": ["source-map-support"]
        }
    },
    "overrides": [{
        "test": ["/underscore.js/", "/websocket.js/"],
        "sourceType": "script"
    },{
        "test": "**/src/ox_modules/**/*.js",
        "comments": true
    }],
    "ignore": [
        "**/src/lib/logger.js"
    ],
    "comments": false
}
